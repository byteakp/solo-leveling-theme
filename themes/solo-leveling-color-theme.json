    },
    "tokenColors": [
      {
        "name": "Comments",
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "foreground": "#4D5673",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Keywords",
        "scope": [
          "keyword",
          "keyword.control",
          "keyword.operator",
          "keyword.other.special-method",
          "keyword.other.unit",
          "storage",
          "storage.type",
          "storage.modifier"
        ],
        "settings": {
          "foreground": "#00AEEF"
        }
      },
      {
        "name": "Function and Class declarations",
        "scope": [
          "entity.name.function",
          "meta.function-call",
          "entity.name.class",
          "entity.name.method"
        ],
        "settings": {
          "foreground": "#00D5FF"
        }
      },
      {
        "name": "Variables and parameters",
        "scope": [
          "variable",
          "variable.parameter",
          "meta.parameter",
          "entity.name.variable"
        ],
        "settings": {
          "foreground": "#50E3FF"
        }
      },
      {
        "name": "Strings and constants",
        "scope": [
          "string",
          "constant.character",
          "constant.language",
          "constant.other"
        ],
        "settings": {
          "foreground": "#A020F0"
        }
      },
      {
        "name": "Numbers",
        "scope": [
          "constant.numeric"
        ],
        "settings": {
          "foreground": "#C050FF"
        }
      },
      {
        "name": "Types",
        "scope": [
          "entity.name.type",
          "support.type"
        ],
        "settings": {
          "foreground": "#50C8FF"
        }
      },
      {
        "name": "Operators",
        "scope": [
          "keyword.operator"
        ],
        "settings": {
          "foreground": "#00AEEF"
        }
      },
      {
        "name": "Punctuation",
        "scope": [
          "punctuation",
          "punctuation.separator",
          "punctuation.terminator",
          "punctuation.definition.parameters"
        ],
        "settings": {
          "foreground": "#B8B8B8"
        }
      },
      {
        "name": "Markup - Headings",
        "scope": [
          "markup.heading"
        ],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#00AEEF"
        }
      },
      {
        "name": "Markup - Bold",
        "scope": [
          "markup.bold"
        ],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#E0E0E0"
        }
      },
      {
        "name": "Markup - Italic",
        "scope": [
          "markup.italic"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#E0E0E0"
        }
      },
      {
        "name": "Markup - Links",
        "scope": [
          "markup.underline.link"
        ],
        "settings": {
          "foreground": "#00AEEF"
        }
      },
      {
        "name": "Error and Invalid",
        "scope": [
          "invalid",
          "invalid.illegal"
        ],
        "settings": {
          "foreground": "#FF3131"
        }
      },
      {
        "name": "Tags",
        "scope": [
          "entity.name.tag",
          "meta.tag.sgml",
          "markup.deleted.git_gutter"
        ],
        "settings": {
          "foreground": "#00AEEF"
        }
      },
      {
        "name": "Attributes",
        "scope": [
          "entity.other.attribute-name"
        ],
        "settings": {
          "foreground": "#A020F0"
        }
      },
      {
        "name": "Built-in functions",
        "scope": [
          "support.function"
        ],
        "settings": {
          "foreground": "#00D5FF"
        }
      },
      {
        "name": "Classes and libraries",
        "scope": [
          "support.class",
          "support.component"
        ],
        "settings": {
          "foreground": "#50C8FF"
        }
      },
      {
        "name": "JSON Keys",
        "scope": [
          "meta.structure.dictionary.json string.quoted.double.json"
        ],
        "settings": {
          "foreground": "#00AEEF"
        }
      },
      {
        "name": "JSON Values",
        "scope": [
          "meta.structure.dictionary.value.json string.quoted.double.json"
        ],
        "settings": {
          "foreground": "#A020F0"
        }
      },
      {
        "name": "CSS Properties",
        "scope": [
          "support.type.property-name.css"
        ],
        "settings": {
          "foreground": "#00AEEF"
        }
      },
      {
        "name": "CSS Values",
        "scope": [
          "support.constant.property-value.css"
        ],
        "settings": {
          "foreground": "#A020F0"
        }
      },
      {
        "name": "HTML Tags",
        "scope": [
          "entity.name.tag.html"
        ],
        "settings": {
          "foreground": "#00AEEF"
        }
      },
      {
        "name": "HTML Attributes",
        "scope": [
          "entity.other.attribute-name.html"
        ],
        "settings": {
          "foreground": "#A020F0"
        }
      }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
      "function": "#00D5FF",
      "class": "#50C8FF",
      "interface": "#50C8FF",
      "enum": "#50C8FF",
      "typeParameter": "#50C8FF",
      "type": "#50C8FF",
      "namespace": "#00AEEF",
      "property": "#50E3FF",
      "variable": "#50E3FF",
      "parameter": "#50E3FF",
      "enumMember": "#C050FF",
      "event": "#00D5FF",
      "method": "#00D5FF",
      "macro": "#FF3131",
      "modifier": "#00AEEF",
      "comment": "#4D5673",
      "string": "#A020F0",
      "keyword": "#00AEEF",
      "number": "#C050FF",
      "regexp": "#A020F0",
      "operator": "#00AEEF"
    }
  }